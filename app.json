{
  "expo": {
    "name": "Rocket Spot",
    "description": "Points at SHAR launch pads from your current location",
    "slug": "rocket-spot",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#142634",
    "icon": "./assets/app_icon.png",
    "splash": {
      "image": "./assets/splash_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#142634"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.messierlabs.rocketspot"
    },
    "android": {
      "package": "com.messierlabs.rocketspot",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "githubUrl": "https://github.com/Messier-Labs/rocket-spot"
  }
}
